{
	"applications": {
		"testapp": {
			"type": "php",
			"user": "unit-testapp",
			"group": "unit-testapp",
			"root": "/var/www/testapp/www/",
			"script": "index.php",
			"working_directory": "/var/www/testapp/",
			"options": {
				"file": "/etc/php/8.1/embed/conf.d/100-testapp.ini",
				"admin": {
					"memory_limit": "256M"
				},
				"user": {
					"display_errors": "0",
					"upload_max_filesize": "10M",
					"post_max_size": "10M",
					"max_execution_time": "10"
				}
			}
		}
	},
	"routes": {
		"testapp-force-https": [
			{
				"match": {
					"host": "testapp.g105b.com",
					"scheme": "http"
				},
				"action": {
					"return": 303,
					"location": "https://testapp.g105b.com"
				}
			}
		],
		"testapp": [
			{
				"match": {
					"host": "testapp.g105b.com"
				},
				"action": {
					"share": "/var/www/testapp/www$uri",
					"chroot": "/var/www/testapp/",
					"follow_symlinks": true,
					"traverse_mounts": true,
					"fallback": {
						"pass": "applications/testapp"
					}
				}
			}
		]
	},
	"listeners": {
		"*:80": {
			"pass": "routes/http"
		},
		"*:443": {
			"pass": "routes/testapp",
			"tls": {
				"certificate": "testapp-certbot"
			}
		}
	},
	"access_log": {
		"path": "/var/log/unit/access.log",
		"format": "$remote_addr - - [$time_local] \"$request_line\" $status $body_bytes_sent \"$header_referer\" \"$header_user_agent\" $cookie_ulid"
	}
}
